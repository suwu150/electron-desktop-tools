{
  "name": "electron-desktop-tool",
  "version": "1.0.0",
  "description": "'A test for electron, a desktop tool'",
  "icon": "./resources/image/logo.png",
  "main": "./main.js",
  "scripts": {
    "electron": "electron .",
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open ./webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack -p --config ./webpack.config.js",
    "packager-linux": "electron-packager ./ MX-WorkStation --overwrite --platform=linux --arch=x64 --out=/home/jkwu/workFiles --ignore=node_modules",
    "packager-win": "electron-packager ./ MX-WorkStation --overwrite --platform=win32 --out=/home/jkwu/workFiles --ignore=node_modules --ignore=app --ignore=OutPut",
    "packager-mac": "electron-packager ./ MX-WorkStation --overwrite --platform=darwin --arch=x64 --out=/home/jkwu/workFiles --ignore=node_modules",
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server --hot --inline --port 10000",
    "hot-server": "cross-env NODE_ENV=development node --max_old_space_size=2096 -r babel-register server.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./src/main.development",
    "parentScript": "cross-env GREET=\"Joe\" npm run childScript",
    "childScript": "echo Hello $GREET"
  },
  "author": "jkwu",
  "license": "ISC",
  "keywords": [
    "Electron",
    "start",
    "demo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suwu150/electronDesktopTool.git"
  },
  "devDependencies": {
    "antd": "^2.8.0",
    "async-es": "^2.5.0",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-import": "^1.2.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.22.0",
    "babel-preset-stage-3": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "cross-env": "^4.0.0",
    "css-loader": "^0.27.3",
    "electron": "^1.6.11",
    "electron-packager": "^8.6.0",
    "eslint": "^3.18.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-loader": "^1.7.0",
    "express": "3.1.0",
    "fs": "0.0.1-security",
    "jsx-loader": "^0.13.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "mongoose": "^4.11.1",
    "nodejieba": "^2.2.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.5",
    "react-router": "^3.x",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "style-loader": "^0.16.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-merge": "^4.1.0",
    "webpack-validator": "^3.0.0",
    "webrtc.io": "latest",
    "webrtc.io-client": "latest",
    "ws": "latest"
  },
  "engines": {
    "node": ">= 0.6.0"
  }
}
